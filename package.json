{
  "name": "node-red-contrib-modbustcp",
  "version": "0.2.0",
  "description": "Node-RED nodes for communicating with a MODBUS TCP Server.",
  "dependencies": {
    "jsmodbus": "^0.5.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Argonne-National-Laboratory/node-red-contrib-modbustcp.git"
  },
  "keywords": [
    "node-red",
    "modbus",
    "modbus tcp",
    "modbustcp",
    "modbus read",
    "modbus write"
  ],
  "node-red": {
    "nodes": {
      "ModbusTCP-Server": "modbustcp/modbustcp-server.js",
      "ModbusTCP-Response": "modbustcp/modbustcp-response.js",
      "ModbusTCP-Read": "modbustcp/modbustcp-read.js",
      "ModbusTCP-Write": "modbustcp/modbustcp-write.js"
    }
  },
  "author": "Jason D. Harper",
  "license": "BSD-3-Clause",
  "bugs": {
    "url": "https://github.com/Argonne-National-Laboratory/node-red-contrib-modbustcp/issues"
  },
  "homepage": "https://github.com/Argonne-National-Laboratory/node-red-contrib-modbustcp#readme",
  "scripts": {
    "test": "karma start modbustcp.conf.js --log-level debug --single-run"
  }
}
